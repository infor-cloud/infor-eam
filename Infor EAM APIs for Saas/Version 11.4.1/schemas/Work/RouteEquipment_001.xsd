<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/RouteEquipment_001" 
	xmlns="http://schemas.datastream.net/MP_entities/RouteEquipment_001" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:dsf="http://schemas.datastream.net/MP_fields" 
	elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" 
		schemaLocation="../MP_fields.xsd"/>
	<!--====================  Begining of RouteEquipment Entity ====================-->
	<xs:element name="RouteEquipment">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:ROUTEEQUIPMENTID"/>
				<xs:element ref="dsf:ROUTEEQUIPMENTTYPE"/>
				<xs:element ref="dsf:FROMPOINT" minOccurs="0"/>
				<xs:element ref="dsf:FROMREFDESC" minOccurs="0"/>
				<xs:element ref="dsf:FROMGEOREF" minOccurs="0"/>
				<xs:element ref="dsf:TOPOINT" minOccurs="0"/>
				<xs:element ref="dsf:TOREFDESC" minOccurs="0"/>
				<xs:element ref="dsf:TOGEOREF" minOccurs="0"/>
				<xs:element ref="dsf:FROMREFERENCEID" minOccurs="0"/>
				<xs:element ref="dsf:FROMOFFSET" minOccurs="0"/>
				<xs:element ref="dsf:FROMOFFSETDIRECTION" minOccurs="0"/>
				<xs:element ref="dsf:TOREFERENCEID" minOccurs="0"/>
				<xs:element ref="dsf:TOOFFSET" minOccurs="0"/>
				<xs:element ref="dsf:TOOFFSETDIRECTION" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
