<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/EAMPackage_001"
		xmlns="http://schemas.datastream.net/MP_entities/EAMPackage_001"
		xmlns:xs="http://www.w3.org/2001/XMLSchema"
		xmlns:dsf="http://schemas.datastream.net/MP_fields"
		elementFormDefault="qualified">
		<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
		<!--===============================================================================-->
		<xs:element name="EAMPackage">
			<xs:complexType>
				<xs:sequence>
					<xs:element ref="dsf:PACKAGEID"/>
					<xs:element ref="dsf:PACKAGENOTE" minOccurs="0"/>
					<xs:element ref="dsf:STATUS"/>
					<xs:element ref="dsf:RSTATUS"/>
					<xs:element ref="dsf:CREATEDBY" minOccurs="0"/>
					<xs:element ref="dsf:CREATEDDATE" minOccurs="0"/>
					<xs:element ref="dsf:UPDATEDBY" minOccurs="0"/>
					<xs:element ref="dsf:DATEUPDATED" minOccurs="0"/>
					<xs:element ref="dsf:CLASSID" minOccurs="0"/>
					<xs:element ref="dsf:USERDEFINEDAREA" minOccurs="0"/>
					<xs:element ref="dsf:StandardUserDefinedFields" minOccurs="0"/>
				</xs:sequence>
				<xs:attribute name="recordid" type="xs:long"/>
			</xs:complexType>
		</xs:element>
</xs:schema>
